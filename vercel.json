{
  "buildCommand": "flutter pub get && flutter build web --release",
  "outputDirectory": "build/web",
  "env": {
    "FLUTTER_MODE": "release"
  },
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, immutable"
        }
      ]
    }
  ]
}